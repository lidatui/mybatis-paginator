<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="db.table.city">

    <select id="findByCity" resultType="map">
        select * from base_city where code = #{code}
    </select>

</mapper>